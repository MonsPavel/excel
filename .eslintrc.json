{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "require-jsdoc": "off",
        "linebreak-style": 0,
        "indent": 0,
        "keyword-spacing": 0,
        "operator-linebreak": "off",
        "object-curly-spacing": 0
    },
    "extends": ["eslint:recommended", "google"]
}
